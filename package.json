{
  "name": "frontcore-server",
  "version": "0.0.0",
  "description": "A web based centralize tool to perform static code analysis for your projects",
  "author": "Ashwin Hegde",
  "scripts": {
    "start:dev": "NODE_ENV=development babel-node ./src/server.js",
    "start:prod": "NODE_ENV=production babel-node ./src/server.js",
    "start:cluster": "NODE_ENV=production NODE_DEBUG=cluster babel-node ./src/cluster.js",
    "start:nodemon": "nodemon ./src/server.js --exec babel-node --presets es2015",
    "start:debug": "NODE_ENV=development babel-node --debug-brk ./src/server.js",
    "nspcheck": "nsp check",
    "lint": "eslint config src",
    "lint:fix": "eslint --fix config src",
		"doc": "jsdoc -d docs src"
  },
  "dependencies": {
    "bcrypt": "^0.8.7",
    "body-parser": "^1.15.0",
    "brace": "^0.8.0",
    "bunyan": "^1.8.5",
    "compression": "^1.5.2",
    "debug": "~2.2.0",
    "elasticsearch": "^16.7.3",
    "es6-promise": "^3.2.1",
    "eslint": "^2.6.0",
    "events": "^1.1.0",
    "express": "^4.13.4",
    "express-session": "^1.14.1",
    "helmet": "^2.1.2",
    "jsdoc": "^3.4.3",
    "jsonfile": "^2.3.1",
    "jsonwebtoken": "^7.1.9",
    "mongoose": "^4.5.10",
    "multer": "^1.2.0",
    "node-dir": "^0.1.11",
    "node-inspector": "^0.12.8",
    "nodemailer": "^1.4.0",
    "object-assign": "^4.1.0",
    "passport": "^0.3.0",
    "passport-jwt": "^2.2.0",
    "socket.io": "^1.4.5",
    "superagent": "^1.8.3",
    "underscore": "^1.8.3",
    "validate.js": "^0.10.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.7.4",
    "babel-jest": "^11.0.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "chai": "^3.5.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.16.0",
    "http-server": "^0.9.0",
    "mocha": "^3.0.2",
    "nodemon": "^1.11.0",
    "nsp": "^2.6.2",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.5",
    "supertest": "^2.0.1"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "repository": "https://github.com/Frontcore/frontcore-server",
  "bugs": {
    "url": "https://github.com/Frontcore/frontcore-server/issues"
  },
  "license": "MIT"
}
